rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /tenants/{tenantId} {
      allow read, create: if true;
      allow update, delete: if request.auth != null;
    }
    match /{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
